<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:local="clr-namespace:Evaluacion3PDV.Views"
       x:Class="Evaluacion3PDV.AppShell"
       Title="Evaluacion3PDV">

    <TabBar>
        <Tab Title="Pinturas Populares">
            <ShellContent ContentTemplate="{DataTemplate local:PopularPaintingsPage}" />
        </Tab>
        <Tab Title="Pinturas Guardadas">
            <ShellContent ContentTemplate="{DataTemplate local:SavedPaintingsPage}" />
        </Tab>
        <Tab Title="Artistas Populares">
            <ShellContent ContentTemplate="{DataTemplate local:PopularArtistsPage}" />
        </Tab>
        <Tab Title="Artistas Guardados">
            <ShellContent ContentTemplate="{DataTemplate local:SavedArtistsPage}" />
        </Tab>
    </TabBar>
</Shell>
